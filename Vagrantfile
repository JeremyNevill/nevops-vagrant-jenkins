# -*- mode: ruby -*-
# vi: set ft=ruby :
Vagrant.configure("2") do |config|

  # Vagrant Box (Centos/7) with Minimal Jenkins Install
  # ===================================================
  config.vm.box = "centos/7"

  config.vm.provider "virtualbox" do |vb|
     vb.gui = true
     vb.memory = "1024"
  end

end
